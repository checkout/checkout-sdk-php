{
    "name": "checkout/checkout-sdk-php",
    "description": "Checkout.com SDK for PHP",
    "homepage": "https://github.com/checkout/checkout-sdk-php",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "checkout.com",
        "payment",
        "gateway",
        "checkout",
        "checkoutcom",
        "GW3",
        "CKO",
        "Reboot",
        "SDK",
        "Library",
        "PHP",
        "API"
    ],
    "authors": [
        {
            "name": "Checkout.com",
            "homepage": "https://github.com/checkout/checkout-sdk-php/graphs/contributors",
            "email": "integrations@checkout.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4.1",
        "symfony/polyfill-php80": "^1.23.1",
        "symfony/deprecation-contracts": "^2.5",
        "ext-fileinfo": "*",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpunit/phpunit": "^9.5.10",
        "symfony/phpunit-bridge": "^6.0.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Checkout\\": "lib/Checkout"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Checkout\\Tests\\": "test/Checkout/Tests/"
        }
    }
}
